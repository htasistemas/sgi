# Connection string in the format: postgres://user:password@host:port/database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/sgi
# Set to "true" if your Postgres provider requires SSL.
DATABASE_SSL=false
# Optional pool configuration
DATABASE_POOL_MAX=10
DATABASE_POOL_IDLE=10000
# Express server port
PORT=3000
# CORS origin allowed. Use * to allow all origins.
CORS_ORIGIN=http://localhost:4200
